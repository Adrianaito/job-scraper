<a class="btn btn-primary" href="/">Back</a>
<% jobs = [] %>
<% @jobs.each do |job| %>
  <% unless job[:warning].downcase == "japanese required" %>
    <% jobs << job %>
  <% end %>
<% end %>
<p><%= jobs.count %></p>
<% jobs.each do |job| %>
  <h1><%= job[:title] %></h1>
  <p><%= job[:company] %></p>
  <p><%= job[:location] %></p>
  <h5><%= job[:warning] %></h5>
  <a class="btn btn-primary" href=<%= job[:url]%>>
    Read more </a>
<% end %>
